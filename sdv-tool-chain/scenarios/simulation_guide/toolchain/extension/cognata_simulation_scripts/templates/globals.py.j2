# hack to be able to share projectID across symphony stages
PROJECT_ID_TEMP_FILE = "project_id.txt"

def write_project_id(projectId):
    temp_file = open(PROJECT_ID_TEMP_FILE, "w")
    temp_file.write(str(projectId))
    temp_file.close()

def get_project_id():
    temp_file = open(PROJECT_ID_TEMP_FILE, "r")
    project_id = temp_file.read()
    temp_file.close()
    return project_id
